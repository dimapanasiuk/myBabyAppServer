{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true,
      "amd": true
    },
    // "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
      "no-var": 1,
      "no-console": 1,
      "no-alert": 1,
      "semi": [2, "always"],
      "quotes": ["error", "double"],
      "no-else-return": 0,
      "indent": ["error", 2, {"SwitchCase": 1}],
      "no-multiple-empty-lines": "error",
      "eol-last": ["error", "always"]
    }
  }